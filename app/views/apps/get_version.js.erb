<% if @response %>
	$that = $('tr#app_<%= @app.id %> .version');
	<% if @app.needs_updated == false %>
		$that.removeClass('alert-error').addClass('alert-success');
	<% end %>
		$that.html("<%= @app.rails_version %>")
<% else %>
	// TODO: do something with this false return instead of console.log
	console.log("need to do something about false return");
<% end %>